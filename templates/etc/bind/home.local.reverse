$TTL	60
@	IN	SOA	localhost. root.localhost. (
		 2025052600		; Serial
			     1h		; Refresh
			    30m		; Retry
			     1w		; Expire
			 604800 )	; Negative Cache TTL
    IN  NS  {{ local_net_prefix }}.254.
;

1	IN	PTR	server.{{ local_domain }}.
5	IN	PTR	streamer.{{ local_domain }}.
20	IN	PTR	media.{{ local_domain }}.
254	IN	PTR	gateway.{{ local_domain }}.
