network:
  version: 2
  ethernets:
    enp1s0:
      addresses:
      - "{{ local_net_prefix }}.2/24"
      nameservers:
        addresses:
        - {{ local_net_prefix }}.254
        search:
        - {{ local_domain }}
      routes:
      - to: "default"
        via: "{{ local_net_prefix }}.254"
