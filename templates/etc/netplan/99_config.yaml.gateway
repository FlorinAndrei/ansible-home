network:
  version: 2
  renderer: networkd
  ethernets:
    eth0:
      addresses:
        - {{ local_net_prefix }}.254/24
      nameservers:
          search: [{{ local_domain }}]
          addresses: [127.0.0.1]
    enp4s0:
      dhcp4: true
      optional: true
      dhcp4-overrides:
        use-dns: false
